{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "AzuredatafactoryMorning001"
		},
		"LS_BlobConnectionm001_properties_typeProperties_connectionString_secretName": {
			"type": "string",
			"defaultValue": "BlobConnectionString"
		},
		"LS_KeyvaultM001_properties_typeProperties_baseUrl": {
			"type": "string",
			"defaultValue": "https://AzurekeyvaultMorning001.vault.azure.net/"
		},
		"LS_adlsgen2sam001_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://adlsgen2sam001.dfs.core.windows.net"
		},
		"LS_azuresqlconnectionm001_properties_typeProperties_connectionString_secretName": {
			"type": "string",
			"defaultValue": "AzureKVSecretMorning001"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "AzuredatafactoryMorning001_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"LS_KeyvaultM001_properties_typeProperties_baseUrl": {
						"value": "[parameters('LS_KeyvaultM001_properties_typeProperties_baseUrl')]"
					},
					"LS_BlobConnectionm001_properties_typeProperties_connectionString_secretName": {
						"value": "[parameters('LS_BlobConnectionm001_properties_typeProperties_connectionString_secretName')]"
					},
					"LS_adlsgen2sam001_properties_typeProperties_url": {
						"value": "[parameters('LS_adlsgen2sam001_properties_typeProperties_url')]"
					},
					"LS_azuresqlconnectionm001_properties_typeProperties_connectionString_secretName": {
						"value": "[parameters('LS_azuresqlconnectionm001_properties_typeProperties_connectionString_secretName')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}